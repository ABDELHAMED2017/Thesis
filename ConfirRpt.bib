@ARTICLE{Fusco2006,
  author = {Fusco, T. and Tanda, M.},
  title = {ML-based symbol timing and frequency offset estimation for OFDM systems
	with noncircular transmissions},
  journal = {IEEE Transactions on Signal Processing},
  year = {2006},
  volume = {54},
  pages = {3527 -3541},
  number = {9},
  month = {sept.},
  abstract = {This paper deals with the problem of joint symbol timing and carrier-frequency
	offset (CFO) estimation in orthogonal frequency-division multiplexing
	(OFDM) systems with noncircular (NC) transmissions. Maximum-likelihood
	(ML) estimators of symbol timing and CFO have been derived under
	the assumption of nondispersive channel and by modeling the OFDM
	signal vector as a circular complex Gaussian random vector (C-CGRV).
	The Gaussian assumption is reasonable when the number of subcarriers
	is sufficiently large. However, if the data symbols belong to an
